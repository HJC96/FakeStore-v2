      "MemberSignUpDTO": {
        "required": [
          "birthDay",
          "birthMonth",
          "birthYear",
          "email",
          "gender",
          "name",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "pattern": "^[a-zA-Z0-9+-\\_.]+@[a-zA-Z0-9-]+\\.[a-zA-Z0-9-.]+$",
            "type": "string"
          },
          "password": {
            "pattern": "^(?=.*[A-Za-z])(?=.*\\d)(?=.*[~!@#$%^&*()+|=])[A-Za-z\\d~!@#$%^&*()+|=]{7,16}$",
            "type": "string"
          },
          "name": {
            "pattern": "^[a-zA-Z가-힣\\\\s]{2,15}",
            "type": "string"
          },
          "birthYear": {
            "pattern": "^\\d{4}$",
            "type": "string"
          },
          "birthMonth": {
            "pattern": "^(0?[1-9]|1[012])$",
            "type": "string"
          },
          "birthDay": {
            "pattern": "^(0?[1-9]|[12][0-9]|3[01])$",
            "type": "string"
          },
          "gender": {
            "pattern": "^[MF]{1}$",
            "type": "string"
          }
        }
      },
