            "example": "john@gmail.com"
          },
          "password": {
            "pattern": "^(?=.*[A-Za-z])(?=.*\\d)(?=.*[~!@#$%^&*()+|=])[A-Za-z\\d~!@#$%^&*()+|=]{7,16}$",
            "type": "string",
            "example": "0000"
          }
        }
      },
